<project>
	<name>Ogmo Editor Example Project</name>
	<settings>
		<defaultWidth>640</defaultWidth>
		<defaultHeight>480</defaultHeight>
		<minWidth>320</minWidth>
		<minHeight>240</minHeight>
		<maxWidth>960</maxWidth>
		<maxHeight>720</maxHeight>
		<workingDirectory>..\\..\\Sprites\\</workingDirectory>
	</settings>
	<values>
		<string name="title" default="new level" maxChars="12" />
	        <integer name="difficulty" default="1" min="1" max="5" />
	</values>
	<tilesets>
		<tileset name="bricks" image="Environments\\tiles_bricks.png" tileWidth="16" tileHeight="16" />
		<tileset name="grass" image="Environments\\tiles_grass.png" tileWidth="16" tileHeight="16" />
	</tilesets>
	<objects>
		<object name="ogmo" image="Items\\ogmo.png" width="16" height="16" limit="1"/>
		<object name="chest" image="Items\\chest.png" width="16" height="16">
			<values>
				<integer name="coins" min="0" max="20" default="2" />
			</values>
		</object>
		<object name="moving_platform" image="Items\\moving_platform.png" width="16" height="16" resizableX="true" resizableY="true" tile="true">
			<values>
				<number name="speed" min="0.1" max="5" default="1" />
			</values>
			<nodes drawObject="true" limit="3" lineMode="2"/>
		</object>
		<object name="platform" image="Items\\moving_platform.png" width="16" height="16" resizableX="true" resizableY="true" tile="true"/>
		<folder name="dangers" image="Items\\spikes_ground.png">
			<object name="spikes_ground" image="Items\\spikes_ground.png" tile="true" width="16" height="16" resizableX="true" />
			<object name="spikes_ceiling" image="Items\\spikes_ceiling.png" tile="true" width="16" height="16" resizableX="true" />
			<object name="spikes_left" image="Items\\spikes_left.png" tile="true" width="16" height="16" resizableY="true" />
			<object name="spikes_right" image="Items\\spikes_right.png" tile="true" width="16" height="16" resizableY="true" />
		</folder>
	</objects>
	<layers>
		<tiles name="tiles_bg" gridSize="16" drawGridSize="64" exportTileIDs="false"/>
		<grid name="floors" gridSize="16" exportAsObjects="false" />
		<objects name="objects" gridSize="16" drawGridSize="32" />
		<tiles name="tiles_floors" gridSize="16" drawGridSize="64" exportTileIDs="true"/>
	</layers>
</project>